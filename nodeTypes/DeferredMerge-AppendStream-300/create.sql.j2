{% if desiredState and desiredState != currentState %}

{% set nsVariables = namespace(taskWhenRun="",tableKey=[],orderBy="", partition="",
 rowNumber="", hybridMergeSortFilter=[], sortKey=[],tableKeySort=[]) %}



        {# Hybrid View Info #}
